server {
  server_name www.tristandunn.com;

  return 301 http://tristandunn.com$request_uri;
}

server {
  root /var/www/tristandunn.com/current/_site;

  listen 80;
  server_name tristandunn.com tristandunn.local;

  charset UTF-8;

  location ~ ^/assets/ {
    expires max;

    gzip_vary on;
    gzip_static on;

    add_header ETag "";
  }
}
